{
  "name": "autonome",
  "version": "1.0.0",
  "description": "Application AutonoME",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "testlinux": "electron --no-sandbox .",
    "package-mac": "electron-packager . AutonoME --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . AutonoME --overwrite --asar --platform=win32 --arch=x64 --icon=includes/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=Nedap --version-string.FileDescription=Nedap-AutonoME --version-string.ProductName=\"AutonoME\"",
    "package-linux": "electron-packager . AutonoME --executableName=autonome --overwrite --no-asar --platform=linux --arch=x64 --icon=resources/img/tt.png --prune=true --out=release-builds"
  },
  "author": "Nedap France",
  "license": "ISC",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^5.0.13"
  },
  "dependencies": {
    "adm-zip": "^0.4.16",
    "axios": "^0.19.2",
    "big-integer": "^1.6.48",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "colors": "^1.4.0",
    "dialog": "^0.3.1",
    "electron-modal": "^1.0.0",
    "electron-packager": "^15.1.0",
    "electron-virtual-keyboard": "^1.0.7",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "js-base64": "^3.5.2",
    "md5": "^2.3.0",
    "moment": "^2.28.0",
    "qrcode": "^1.4.4",
    "randomstring": "^1.1.5",
    "reconnecting-websocket": "^4.4.0",
    "request": "^2.88.2",
    "requestretry": "^4.1.1",
    "retry-axios": "^2.2.1",
    "simple-keyboard": "^2.30.31",
    "socket.io": "^2.3.0",
    "speak-tts": "^2.0.8",
    "sweetalert2": "^8.19.0",
    "unzipper": "^0.10.11"
  }
}
